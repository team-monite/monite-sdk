{
  "name": "@team-monite/sdk-demo",
  "version": "1.12.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./build/esm/index.js",
    "./package.json": "./package.json",
    "./*": "./build/*.js"
  },
  "types": "build/src/index.d.ts",
  "scripts": {
    "dev": "vite --port 5173",
    "dev:lib": "cross-env NODE_ENV=development rollup --watch --config rollup.config.mjs",
    "dev:with-lib": "yarn dev:lib & vite --port 5173",
    "build": "vite build --outDir build && yarn build:lib",
    "build:lib": "cross-env NODE_ENV=production rollup --config rollup.config.mjs --silent",
    "clean": "rimraf build/; rimraf --glob 'src/locales/*/messages.ts'; rimraf --glob 'src/locales/*/messages.d.ts'",
    "preview": "vite preview --outDir build",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "extract-translations": "lingui extract --config lingui.config.cjs",
    "compile-translations": "lingui compile --config lingui.config.cjs"
  },
  "sideEffects": false,
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@mui/icons-material": "~5.14.3",
    "@mui/material": "~5.15.14",
    "@openapi-qraft/react": "1.14.0",
    "@tanstack/query-core": "~5.28.6",
    "@tanstack/react-query": "~5.28.6",
    "@tanstack/react-query-devtools": "~5.28.6",
    "vite-plugin-html-config": "~2.0.2"
  },
  "devDependencies": {
    "@hookform/resolvers": "~2.9.3",
    "@lingui/cli": "^5.3.0",
    "@lingui/core": "^5.3.0",
    "@lingui/macro": "^5.3.0",
    "@lingui/react": "^5.3.0",
    "@monite/sdk-react": "workspace:~",
    "@team-monite/eslint-plugin": "workspace:~",
    "@team-monite/rollup-config": "workspace:~",
    "@types/cookie-parser": "~1.4.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-helmet": "^6.1.5",
    "@vitejs/plugin-react-swc": "~3.7.1",
    "babel-plugin-macros": "^3.1.0",
    "cookie-parser": "~1.4.7",
    "cross-env": "^7.0.3",
    "deepmerge": "~4.3.1",
    "eslint": "~8.57.0",
    "eslint-import-resolver-typescript": "~4.3.5",
    "express": "~4.21.2",
    "lodash-es": "~4.17.21",
    "nanoid": "~5.0.9",
    "node-fetch": "~3.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "~7.33.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.3.2",
    "rimraf": "~5.0.0",
    "rollup": "~3.29.5",
    "sass": "^1.52.1",
    "ts-node": "~10.9.1",
    "typescript": "~5.5.4",
    "vite": "~5.4.19",
    "vite-plugin-dts": "~4.5.4",
    "web-vitals": "^2.1.4",
    "yup": "~0.32.11"
  }
}
