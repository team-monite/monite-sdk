  "name": "@monite/sdk-react",
  "version": "4.1.0",
  "apiVersion": "2024-05-25",
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "mui-styles.d.ts",
    "setup-tests.cjs"
  ],
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./mui-styles": {
      "types": "./mui-styles.d.ts",
      "import": "./mui-styles.d.ts",
      "require": "./mui-styles.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "yarn run -T vitest run --coverage --silent --no-watch",
    "test:watch": "yarn run -T vitest",
    "test:ui": "yarn run -T vitest --ui",
    "test:changed": "yarn run -T vitest run related $(git diff --name-only --relative origin/main...HEAD)",
    "typecheck": "tsc",
    "build": "cross-env NODE_ENV=production rollup --config config/rollup.config.mjs",
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development rollup --watch --config config/rollup.config.mjs",
    "clean": "rimraf dist/; rimraf --glob 'src/core/i18n/locales/*/messages.ts'; rimraf --glob 'src/core/i18n/locales/*/messages.d.ts'",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs --max-warnings 500",
    "extract-translations": "lingui extract",
    "compile-translations": "node scripts/run-lingui-compile.js",
    "write-package-version-file": "yarn exec ../../write-package-version-file.sh",
    "storybook": "NODE_OPTIONS=\"--max-old-space-size=4096\" storybook dev -p 6006 --debug-webpack",
    "build-storybook": "NODE_OPTIONS=\"--max-old-space-size=4096\" storybook build -o storybook_static --debug-webpack",
    "download-openapi-document": "yarn exec ./download-openapi-document.sh",
    "codegen": "yarn codegen:api-version && yarn exec ./generate-api-client.sh",
    "codegen:api-version": "echo \"// This file was generated automatically \n export const apiVersion = '$(node -p 'require(\"@monite/sdk-react/package.json\").apiVersion')';\" > src/api/api-version.ts"
  },
