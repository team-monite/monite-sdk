{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    },
    "experimental": {
      "plugins": [
        ["@lingui/swc-plugin", {}]
      ]
    }
  },
  "sourceMaps": true
}