{
  "name": "sdk-demo-with-nextjs-and-clerk-auth",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "apiVersion": "2024-05-25",
  "scripts": {
    "dev": "next dev --port 3000 & dotenv -e .env.local -- npm run webhook-relay",
    "build": "next build && yarn build:demo-data-generator",
    "start": "next start --port 3000",
    "lint": "next lint",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "webhook-relay": "smee --url $CLERK_WEBHOOK_DEV_RELAY_URL --port 3000 --path /api/in/clerk",
    "build:demo-data-generator": "tsup-node ./src/lib/monite-api/demo-data-generator/bin.ts --format esm --target esnext --sourcemap",
    "demo-data-generator": "node dist/bin.js",
    "extract-translations": "lingui extract --config lingui.config.cjs",
    "compile-translations": "lingui compile --config lingui.config.cjs",
    "download-openapi-document": "yarn exec ./download-openapi-document.sh",
    "codegen": "yarn codegen:api-version && yarn exec ./generate-api-client.sh",
    "codegen:api-version": "echo \"// This file was generated automatically\nexport const apiVersion = '$(node -p 'require(\"@monite/sdk-react/package.json\").apiVersion')';\" > src/lib/monite-api/api-version.ts"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "4.13.21",
    "@clerk/nextjs": "4.31.4",
    "@emotion/cache": "~11.11.0",
    "@lingui/core": "~4.7.0",
    "@lingui/macro": "~4.7.0",
    "@lingui/react": "~4.7.0",
    "@monite/sdk-api": "workspace:~",
    "@monite/sdk-react": "workspace:~",
    "@mui/icons-material": "~5.14.3",
    "@mui/material": "~5.15.14",
    "@mui/material-nextjs": "^5.15.11",
    "@team-monite/sdk-demo": "workspace:~",
    "@team-monite/sdk-themes": "workspace:~",
    "@types/node": "20.3.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "aedes": "~0.50.1",
    "commander": "~11.1.0",
    "eslint": "8.57.0",
    "eslint-config-next": "~14.0.3",
    "mqtt": "~5.3.1",
    "next": "~14.2.10",
    "openapi-fetch": "~0.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svix": "~1.13.0",
    "typescript": "~5.5.4"
  },
  "devDependencies": {
    "@faker-js/faker": "~8.3.1",
    "@lingui/cli": "~4.7.0",
    "@lingui/swc-plugin": "4.0.4",
    "@openapi-qraft/cli": "1.14.0",
    "@testing-library/jest-dom": "~6.1.4",
    "@testing-library/react": "~14.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "chalk": "~5.3.0",
    "dotenv": "^16.3.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "prettier": "^2.8.8",
    "smee-client": "~2.0.0",
    "tsup": "~8.0.1"
  },
  "packageManager": "yarn@3.6.4"
}
