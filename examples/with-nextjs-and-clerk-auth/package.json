{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000 & dotenv -e .env.local -- npm run webhook-relay",
    "build": "next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "test": "jest --watch",
    "webhook-relay": "smee --url $CLERK_WEBHOOK_DEV_RELAY_URL --port 3000 --path /api/in/clerk",
    "codegen": "openapi-typescript https://api.dev.monite.com/openapi.json?version=$(node -p 'require(\"@monite/sdk-api/package.json\").apiVersion') --output ./src/lib/monite-api/schema.d.ts && prettier --write ./src/lib/monite-api/schema.d.ts",
    "build:demo-data-generator": "tsup-node ./src/lib/monite-api/demo-data-generator/bin.ts --format esm --target esnext --sourcemap",
    "demo-data-generator": "node dist/bin.js"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "~4.13.6",
    "@clerk/nextjs": "~4.29.3",
    "@emotion/cache": "~11.11.0",
    "@emotion/react": "~11.11.1",
    "@emotion/styled": "~11.11.0",
    "@monite/sdk-api": "^3.8.0",
    "@monite/sdk-react": "^3.8.0",
    "@mui/icons-material": "~5.14.3",
    "@mui/material": "~5.14.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "aedes": "~0.50.1",
    "commander": "~11.1.0",
    "eslint": "8.43.0",
    "eslint-config-next": "~14.0.3",
    "mqtt": "~5.3.1",
    "next": "~14.0.3",
    "openapi-fetch": "~0.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svix": "~1.13.0",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "~8.3.1",
    "@testing-library/jest-dom": "~6.1.4",
    "@testing-library/react": "~14.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "chalk": "~5.3.0",
    "dotenv": "^16.3.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "openapi-typescript": "~6.7.4",
    "prettier": "^2.8.8",
    "smee-client": "~2.0.0",
    "tsup": "~8.0.1"
  },
  "packageManager": "yarn@3.6.4"
}
