{
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrder": [
    "^react(.*)$",
    "^[a-z](.*)$",
    "^@[a-z](.*)$",
    "^@/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "importOrderParserPlugins": ["importAssertions", "typescript", "jsx"]
}
